{
  "manifest_version": 2,
  "name": "get-fb-share-list",
  "description": "Get Facebook shared posts users list",
  "version": "0.1.0",
  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  },
  "page_action": {
    "default_title": "Get Facebook shared posts list"
  },
  "permissions": [
    "https://*.facebook.com/shares/*",
    "activeTab",
    "declarativeContent",
    "runtime",
    "pageAction",
    "tabs",
    "extension"
  ]
}
