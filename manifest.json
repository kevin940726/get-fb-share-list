{
  "manifest_version": 2,
  "name": "get-fb-share-list",
  "description": "Get Facebook shared posts list",
  "version": "0.1.0",
  "content_security_policy":
    "script-src 'self' 'unsafe-eval' https://connect.facebook.net https://graph.facebook.com; object-src 'self'",
  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  },
  "page_action": {
    "default_title": "Get Facebook shared posts list"
  },
  "permissions": [
    "https://*.facebook.com/*",
    "activeTab",
    "declarativeContent",
    "tabs"
  ]
}
